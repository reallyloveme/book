{
  "root": "./docs",
  "title": "个人知识记录",
  "author": "songjun",
  "description": "This is a sample book created by gitbook",
  "links": {
      "sharing": {
          "all": null
      },
      "sidebar": {
        "github": "https://github.com/reallyloveme/book"
      }
  },
  "plugins": [
    "multipart",
    "highlight",
    "-search",
    "search-pro",
    "sharing",
    "livereload",
    "chapter-fold",
    "splitter",
    "anchor-navigation-ex",
    "lightbox",
    "hide-element",
    "tbfed-pagefooter",
    "copy-code-button"
  ],
  "pluginsConfig": {
    "expandable-chapters-small":{},
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": true
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "tbfed-pagefooter": {
      "copyright":"",
      "modify_label": "该文件最后修改时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}
